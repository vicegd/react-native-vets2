      "bundleIdentifier": "com.vicegd.reactnativevets2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.vicegd.reactnativevets2"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "eb98ff12-e58b-4895-87dc-9d2455229bb3"
      }
    }
  }
}
